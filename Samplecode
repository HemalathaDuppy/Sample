#include<stdio.h>
int main()
{
 int i,n,a[100],key,flag;
 printf("Enter the no.of elements of array:");
 scanf("%d",&n);
 printf("Enter the elements into the array:");
 for(i=0;i<n;i++)
 {
  scanf("%d",&a[i]);
  printf("Enter the key elements of the array:");
  scanf("%d",&key);
 }
  for(i=0;i<n;i++)
  {
  if(key==a[i])
  {
   flag=1;
   printf("Key is found at %d=i+1",i+1);
   break;
  }
  }
  if(flag!=0)
  {
   printf("Key is not found");
  }
return 0;
}

   
